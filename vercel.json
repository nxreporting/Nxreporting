{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "cd frontend && npm run vercel-build",
  "devCommand": "cd frontend && npm run dev",
  "installCommand": "cd frontend && npm install",
  "outputDirectory": "frontend/.next",
  "functions": {
    "frontend/pages/api/extract.js": {
      "maxDuration": 50
    },
    "frontend/pages/api/files/**/*.js": {
      "maxDuration": 30
    },
    "frontend/pages/api/data/**/*.js": {
      "maxDuration": 30
    },
    "frontend/pages/api/reports/**/*.js": {
      "maxDuration": 30
    },
    "frontend/pages/api/**/*.js": {
      "maxDuration": 10
    }
  },
  "regions": ["iad1"]
}