{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "JWT_SECRET": "@jwt_secret",
    "NANONETS_API_KEY": "@nanonets_api_key"
  },
  "build": {
    "env": {
      "DATABASE_URL": "@database_url"
    }
  }
}